{
  "name": "astha-thermal-print-server",
  "version": "1.0.0",
  "description": "Automatic thermal printer server for Astha HJB Canteen orders",
  "main": "index.js",
  "scripts": {
    "prestart": "node -e \"try { require('fs').unlinkSync('printer'); } catch(e) {}\"",
    "start": "node index.js",
    "dev": "nodemon index.js",
    "build": "pkg . --targets node18-win-x64 --output dist/astha-printer.exe",
    "pretest-print": "node -e \"try { require('fs').unlinkSync('printer'); } catch(e) {}\"",
    "test-print": "node test-printer.js"
  },
  "keywords": [
    "thermal-printer",
    "pos",
    "auto-print",
    "restaurant"
  ],
  "author": "Astha HJB",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "dotenv": "^16.4.1",
    "node-thermal-printer": "^4.4.4",
    "chalk": "^4.1.2",
    "node-notifier": "^10.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "pkg": "^5.8.1"
  }
}
